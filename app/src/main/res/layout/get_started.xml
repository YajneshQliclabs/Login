<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools">

    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumnellipsetwo"
        android:layout_height="match_parent"
        android:layout_marginTop="22.55dp"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearAccounting"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700">
            <View
                android:background="@drawable/rectangle_bg_gray_100_90_radius_30_5"
                android:id="@+id/viewEllipseTwo"
                android:layout_gravity="start"
                android:layout_height="64.44dp"
                android:layout_marginEnd="15.85dp"
                android:layout_marginStart="15.85dp" android:layout_width="64.44dp"/>
            <FrameLayout android:id="@+id/frameStackellipsethree"
                android:layout_gravity="center_horizontal"
                android:layout_height="329.03dp"
                android:layout_marginEnd="6.34dp"
                android:layout_marginStart="6.34dp"
                android:layout_marginTop="12.14dp" android:layout_width="399.32dp">
                <View
                    android:background="@drawable/rectangle_bg_blue_50_radius_17"
                    android:id="@+id/viewEllipseThree"
                    android:layout_gravity="top|end"
                    android:layout_height="35.92dp"
                    android:layout_marginBottom="8.67dp"
                    android:layout_marginEnd="38.03dp"
                    android:layout_marginStart="38.03dp" android:layout_width="35.92dp"/>
                <ImageView android:id="@+id/imageOtherpostsTen"
                    android:layout_gravity="start"
                    android:layout_height="316.56dp"
                    android:layout_marginTop="3.47dp"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    android:imageRes="@{@drawable/img_otherposts10}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_otherposts10"/>
            </FrameLayout>
            <FrameLayout android:gravity="start"
                android:id="@+id/frameStackvectorfive"
                android:layout_height="303.55dp"
                android:layout_marginTop="21.68dp" android:layout_width="match_parent">

                <ImageView
                    android:id="@+id/imageVectorFive"
                    android:layout_width="match_parent"
                    android:layout_height="369.74dp"
                    android:layout_gravity="start"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector5"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_vector5" />

                <LinearLayout
                    android:id="@+id/linearColumnellipsefour"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="top|end"
                    android:layout_marginStart="14.79dp"
                    android:layout_marginEnd="14.79dp"
                    android:layout_marginBottom="8.67dp"
                    android:orientation="vertical">

                    <View
                        android:id="@+id/viewEllipseFour"
                        android:layout_width="23.24dp"
                        android:layout_height="23.24dp"
                        android:layout_gravity="end"
                        android:layout_marginStart="10.56dp"
                        android:background="@drawable/rectangle_bg_cyan_50_radius_11" />

                    <ImageView
                        android:id="@+id/imageMenu"
                        android:layout_width="63.38dp"
                        android:layout_height="8.45dp"
                        android:layout_gravity="start"
                        android:layout_marginStart="135.22dp"
                        android:layout_marginTop="31.22dp"
                        android:layout_marginEnd="135.22dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_menu"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_menu" />

                    <TextView
                        android:id="@+id/txtAccounting"
                        style="@style/txtRobotoromanmedium24"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="start"
                        android:layout_marginStart="98.25dp"
                        android:layout_marginTop="31.22dp"
                        android:layout_marginEnd="98.25dp"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@{accountingVM.accountingModel.txtAccounting}"
                        tools:text="@string/lbl_accounting" />

                    <TextView
                        android:id="@+id/txtDescription"
                        style="@style/txtRobotoromanlight16_1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="29.58dp"
                        android:layout_marginTop="13.88dp"
                        android:layout_marginEnd="29.58dp"
                        android:gravity="start"
                        android:text="@{accountingVM.accountingModel.txtDescription}"
                        tools:text="Work life balance can be achieved by balancing everything in accounting." />

                    <LinearLayout
                        android:id="@+id/linearRowskip"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="66.78dp"
                        android:layout_marginEnd="8.45dp"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/txtSkip"
                            style="@style/txtRobotoromanmedium16"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="15.62dp"
                            android:layout_marginBottom="15.62dp"
                            android:layout_weight="0.25"
                            android:ellipsize="end"
                            android:singleLine="true"
                            android:text="@{accountingVM.accountingModel.txtSkip}"
                            tools:text="@string/lbl_skip" />

                        <Button
                            android:id="@+id/btnNext"
                            style="@style/btnSolidRounded_2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="220.79dp"
                            android:ellipsize="end"
                            android:gravity="center"
                            android:paddingStart="31.69dp"
                            android:paddingTop="16.48dp"
                            android:paddingEnd="31.69dp"
                            android:paddingBottom="16.48dp"
                            android:singleLine="true"
                            android:text="@string/lbl_next"
                            android:textAllCaps="false"
                            tools:text="Next" />
                    </LinearLayout>
                </LinearLayout>
            </FrameLayout>
        </LinearLayout>
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>