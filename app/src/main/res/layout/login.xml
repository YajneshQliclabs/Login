<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools">
    <FrameLayout android:id="@+id/frameLogin"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumnellipsefive"
            android:layout_gravity="top|center"
            android:layout_height="match_parent"
            android:layout_marginBottom="12.14dp"
            android:layout_marginStart="10.56dp"
            android:layout_marginTop="12.14dp"
            android:layout_width="match_parent" android:orientation="vertical">
            <LinearLayout android:id="@+id/linearRowellipsefive"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <FrameLayout android:id="@+id/frameStackellipsefive"
                    android:layout_height="@dimen/_117pxv"
                    android:layout_marginBottom="@dimen/_20pxv" android:layout_width="@dimen/_260pxh">
                    <View
                        android:background="@drawable/rectangle_border_gray_200_91_radius_51"
                        android:id="@+id/viewEllipseFive"
                        android:layout_gravity="top|start"
                        android:layout_height="@dimen/_102pxh"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_10pxh" android:layout_width="@dimen/_102pxh"/>
                    <ImageView android:id="@+id/imageEpriseLogo"
                        android:layout_gravity="bottom|end"
                        android:layout_height="@dimen/_65pxh"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="@dimen/_162pxh"
                        android:scaleType="fitXY"
                        android:imageRes="@{@drawable/img_epriselogo}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_epriselogo"/>
                </FrameLayout>
                <ImageView android:id="@+id/imageEllipseThree"
                    android:layout_height="@dimen/_74pxh"
                    android:layout_marginStart="@dimen/_60pxh"
                    android:layout_marginTop="@dimen/_63pxv"
                    android:layout_width="@dimen/_59pxh"
                    android:scaleType="fitXY"
                    android:imageRes="@{@drawable/img_ellipse3}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse3"/>
            </LinearLayout>
            <TextView android:ellipsize="end" android:id="@+id/txtLogin"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_147pxh"
                android:layout_marginStart="@dimen/_147pxh"
                android:layout_marginTop="@dimen/_4pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{loginVM.loginModel.txtLogin}"
                style="@style/txtRobotoromanlight24" tools:text="@string/lbl_log_in"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowellipsetwo"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_8pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <View
                    android:background="@drawable/rectangle_bg_blue_52_radius_21_5"
                    android:id="@+id/viewEllipseTwo"
                    android:layout_height="@dimen/_43pxh"
                    android:layout_marginBottom="@dimen/_193pxv" android:layout_width="@dimen/_43pxh"/>
                <FrameLayout android:id="@+id/frameStackellipsefour"
                    android:layout_height="@dimen/_206pxv"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_marginTop="@dimen/_30pxv" android:layout_width="@dimen/_223pxh">
                    <View
                        android:background="@drawable/rectangle_bg_cyan_50_radius_11"
                        android:id="@+id/viewEllipseFour"
                        android:layout_gravity="bottom|start"
                        android:layout_height="@dimen/_22pxh"
                        android:layout_marginBottom="@dimen/_26pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_26pxv" android:layout_width="@dimen/_22pxh"/>
                    <ImageView android:id="@+id/imageOtherpostsThirteen"
                        android:layout_gravity="end"
                        android:layout_height="@dimen/_206pxh"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_width="@dimen/_204pxh"
                        android:scaleType="fitXY"
                        android:imageRes="@{@drawable/img_otherposts13}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_otherposts13"/>
                </FrameLayout>
                <View
                    android:background="@drawable/rectangle_border_gray_202_radius_20_5"
                    android:id="@+id/viewEllipseSix"
                    android:layout_height="@dimen/_41pxh"
                    android:layout_marginBottom="@dimen/_62pxv"
                    android:layout_marginStart="@dimen/_27pxh"
                    android:layout_marginTop="@dimen/_133pxv" android:layout_width="@dimen/_41pxh"/>
            </LinearLayout>
            <EditText
                android:background="@drawable/rectangle_bg_white_a700_border_light_blue_a200_radius_5"
                android:ellipsize="end"
                android:hint="@string/lbl_username"
                android:id="@+id/etGroupFour" android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_15pxh"
                android:layout_marginStart="@dimen/_15pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_24pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_24pxh"
                android:paddingTop="@dimen/_23pxv"
                android:singleLine="true"
                android:text="@={loginVM.loginModel.etGroupFourValue}"
                android:textColorHint="@color/black_900"
                style="@style/etSolidRoundedOutline" tools:ignore="Autofill"/>
            <EditText
                android:background="@drawable/rectangle_bg_white_a700_border_light_blue_a200_radius_5"
                android:ellipsize="end"
                android:hint="@string/lbl_password"
                android:id="@+id/etGroupThree"
                android:inputType="textPassword"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_15pxh"
                android:layout_marginStart="@dimen/_15pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_26pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_24pxh"
                android:paddingTop="@dimen/_21pxv"
                android:singleLine="true"
                android:text="@={loginVM.loginModel.etGroupThreeValue}"
                android:textColorHint="@color/black_900"
                style="@style/etSolidRoundedOutline" tools:ignore="Autofill"/>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnLogInOne"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_15pxh"
                android:layout_marginStart="@dimen/_15pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_17pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_17pxv"
                android:singleLine="true"
                android:text="@string/lbl_log_in2"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_2" tools:text="@string/lbl_log_in2"/>
        </LinearLayout>
    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>